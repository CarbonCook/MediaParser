{
  "manifest_version": 2,

  "name": "Download Selected Links",
  "description": "Select links on a page and download them.",
  "version": "1.0",
  "minimum_chrome_version": "16.0.884",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Media Parser"
  },
  "permissions": [
    "activeTab",
    "downloads",
    "<all_urls>"
  ]
}
